<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.AleksandraAndPawel.transportcompany.model.ReviewsEntity" table="reviews" schema="systransport">
        <id name="reviewId" column="review_id"/>
        <property name="stars" column="stars"/>
        <property name="content" column="content"/>
        <property name="packageId" column="package_id"/>
        <many-to-one name="packagesByPackageId" class="com.AleksandraAndPawel.transportcompany.model.PackagesEntity">
            <column name="package_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>